<!DOCTYPE html>
<html lang="fr">

<body>
	Page de Live
	<% if(typeof activity !== "undefined") { %>
    <div class="form-group">
        <label for="register-run" class="col-sm-2 control-label">Postes</label>
        <div class="col-sm-10">
            <select id="register-run" name="registerRun" multiple class="form-control">
                <% activity.forEach(function(item){ %>
                  <option value='<%-item.id%>'> <%-item.description%> </option>
                <% }); %>
            </select>
        </div>
    </div>
    <% } %>

	<div class="raids_container">

	</div>
	<script type="text/javascript">
		funtion updates() {
