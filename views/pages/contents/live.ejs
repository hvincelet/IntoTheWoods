		        $("ul").empty();

		        $.each(data.result, function() {

		        	$("ul").append("<li>Name: "+this['name']+"</li><li>Age: "+this['age']+"</li><li>Company: "+this['company']+"</li><br />");

		        });

	    	});
	    }
	</script>
